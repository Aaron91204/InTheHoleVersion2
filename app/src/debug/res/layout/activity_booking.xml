<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.aaron.inthehole.Booking"
    android:background="#8BC34A">

    <TextView
        android:id="@+id/bookingtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp"
        android:text="Booking Area"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/booking9am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/booking1115am"
        android:layout_alignBottom="@+id/booking1115am"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/rounded_button"
        android:text="9am" />

    <Button
        android:id="@+id/booking915am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/booking1130am"
        android:layout_alignBottom="@+id/booking1130am"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/rounded_button"
        android:text="9:15am" />

    <Button
        android:id="@+id/booking930am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/booking1145am"
        android:layout_alignBottom="@+id/booking1145am"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/rounded_button"
        android:text="9:30am" />

    <Button
        android:id="@+id/booking945am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/booking12pm"
        android:layout_alignBottom="@+id/booking12pm"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/rounded_button"
        android:text="9:45am" />

    <Button
        android:id="@+id/booking10am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/booking1215pm"
        android:layout_alignBottom="@+id/booking1215pm"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/rounded_button"
        android:text="10am" />

    <Button
        android:id="@+id/booking1015am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/booking1230pm"
        android:layout_alignBottom="@+id/booking1230pm"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/rounded_button"
        android:text="10:15am" />

    <Button
        android:id="@+id/booking1030am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/booking1245pm"
        android:layout_alignBottom="@+id/booking1245pm"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/rounded_button"
        android:text="10:30am" />

    <Button
        android:id="@+id/booking1045am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/booking1pm"
        android:layout_alignBottom="@+id/booking1pm"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/rounded_button"
        android:text="10:45am" />

    <Button
        android:id="@+id/booking11am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/profilebtn"
        android:layout_alignBottom="@+id/profilebtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/rounded_button"
        android:text="11am" />

    <Button
        android:id="@+id/booking1115am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/booking1130am"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_button"
        android:text="11:15am" />

    <Button
        android:id="@+id/booking1130am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/booking1145am"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="12dp"
        android:background="@drawable/rounded_button"
        android:text="11:30am" />

    <Button
        android:id="@+id/booking1145am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/booking12pm"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="11dp"
        android:background="@drawable/rounded_button"
        android:text="11:45am" />

    <Button
        android:id="@+id/booking1215pm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/booking1230pm"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="12dp"
        android:background="@drawable/rounded_button"
        android:text="12:15pm" />

    <Button
        android:id="@+id/booking12pm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/booking1215pm"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="12dp"
        android:background="@drawable/rounded_button"
        android:text="12pm" />

    <Button
        android:id="@+id/booking1230pm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/booking1245pm"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_button"
        android:text="12:30pm" />

    <Button
        android:id="@+id/booking1245pm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/booking1pm"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="9dp"
        android:background="@drawable/rounded_button"
        android:text="12:45pm" />

    <Button
        android:id="@+id/booking1pm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/booking115pm"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="12dp"
        android:background="@drawable/rounded_button"
        android:text="1pm" />

    <Button
        android:id="@+id/booking115pm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/rounded_button"
        android:text="1:15pm" />

    <Button
        android:id="@+id/profilebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="Profile"
        android:background="@drawable/rounded_button"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="150sp"
        android:layout_height="match_parent"
        android:layout_alignTop="@+id/booking1115am"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:text="Please select a time that you will like to play."
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25sp" />
</RelativeLayout>
